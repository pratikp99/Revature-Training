package com.revature.emp;

public class Vehicle {
	public static void main(String args[])
	{
		PetrolVehicle v=new PetrolVehicle();
		v.setvNumber(9810);
		v.setDistanceTravelled(1200);
		System.out.println(v);
		System.out.println(v.calculateExpense(v.getDistanceTravelled()));
		
		DieselVehicle d=new DieselVehicle();
		d.setDistanceTravelled(1400);
		d.setvNumber(9914);
		System.out.println(d);
		System.out.println(d.calculateExpense(d.getDistanceTravelled()));
		
		EVehicle e=new EVehicle();
		System.out.println(e.getBenefits());
	}
}
